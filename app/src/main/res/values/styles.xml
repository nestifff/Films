<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="mySmallRatingBarTheme" parent="android:style/Widget.Material.RatingBar.Small">
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/pink</item>
    </style>

    <style name="EditTextSearchStyle" parent="android:Widget.EditText">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textColorHint">#535353</item>
        <item name="colorControlActivated">@color/pink</item>
        <item name="android:textSize">16sp</item>
        <item name="android:backgroundTint">@color/pink</item>
        <item name="android:textCursorDrawable">@null</item>
    </style>
</resources>
